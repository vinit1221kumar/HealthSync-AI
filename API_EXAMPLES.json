{
  "auth": {
    "signup": {
      "method": "POST",
      "url": "http://localhost:5000/api/auth/signup",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {
        "name": "John Doe",
        "email": "john@example.com",
        "password": "password123",
        "age": 25,
        "gender": "male",
        "height": 175,
        "weight": 70
      }
    },
    "login": {
      "method": "POST",
      "url": "http://localhost:5000/api/auth/login",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {
        "email": "john@example.com",
        "password": "password123"
      }
    },
    "getMe": {
      "method": "GET",
      "url": "http://localhost:5000/api/auth/me",
      "headers": {
        "Authorization": "Bearer YOUR_TOKEN_HERE"
      }
    }
  },
  "health": {
    "createData": {
      "method": "POST",
      "url": "http://localhost:5000/api/health",
      "headers": {
        "Authorization": "Bearer YOUR_TOKEN_HERE",
        "Content-Type": "application/json"
      },
      "body": {
        "date": "2026-01-27",
        "steps": 10000,
        "sleepHours": 8,
        "waterIntake": 3,
        "calories": 2000,
        "mood": "excellent",
        "notes": "Great day!"
      }
    },
    "getData": {
      "method": "GET",
      "url": "http://localhost:5000/api/health?limit=30",
      "headers": {
        "Authorization": "Bearer YOUR_TOKEN_HERE"
      }
    },
    "getStats": {
      "method": "GET",
      "url": "http://localhost:5000/api/health/stats/summary?days=7",
      "headers": {
        "Authorization": "Bearer YOUR_TOKEN_HERE"
      }
    }
  },
  "ai": {
    "generateAnalysis": {
      "method": "POST",
      "url": "http://localhost:5000/api/ai/analyze",
      "headers": {
        "Authorization": "Bearer YOUR_TOKEN_HERE",
        "Content-Type": "application/json"
      },
      "body": {
        "days": 7
      }
    },
    "getLatest": {
      "method": "GET",
      "url": "http://localhost:5000/api/ai/latest",
      "headers": {
        "Authorization": "Bearer YOUR_TOKEN_HERE"
      }
    }
  }
}
