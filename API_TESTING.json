    {
      "name": "Auth - Signup",
      "request": {
        "url": "http://localhost:5000/api/auth/signup",
        "method": "POST",
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "raw": "{\n  \"name\": \"John Doe\",\n  \"email\": \"john@example.com\",\n  \"password\": \"password123\",\n  \"age\": 28,\n  \"gender\": \"male\",\n  \"height\": 180,\n  \"weight\": 75\n}"
        }
      }
    },
    {
      "name": "Auth - Login",
      "request": {
        "url": "http://localhost:5000/api/auth/login",
        "method": "POST",
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "raw": "{\n  \"email\": \"john@example.com\",\n  \"password\": \"password123\"\n}"
        }
      }
    },
    {
